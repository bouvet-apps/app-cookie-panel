    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.23.6",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "babel-eslint": "10.1.0",
    "concurrently": "4.1.2",
    "deepmerge": "4.3.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
